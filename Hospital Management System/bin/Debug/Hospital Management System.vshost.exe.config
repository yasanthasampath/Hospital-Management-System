<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IHospitalService1" />
                <binding name="BasicHttpBinding_IBillService" />
                <binding name="BasicHttpBinding_INurseService" />
                <binding name="BasicHttpBinding_IBillService1" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8080/NurseService" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IHospitalService1"
                contract="BillService.IHospitalService" name="BasicHttpBinding_IHospitalService1" />
            <endpoint address="http://localhost:8080/BillService" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IBillService" contract="BillService.IBillService"
                name="BasicHttpBinding_IBillService" />
            <endpoint address="http://localhost:8080/NurseService" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_INurseService" contract="NurseService.INurseService"
                name="BasicHttpBinding_INurseService" />
            <endpoint address="http://localhost:8080/BillService" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IBillService1" contract="NurseService.IBillService"
                name="BasicHttpBinding_IBillService1" />
        </client>
    </system.serviceModel>
</configuration>